[32m+[m[32m#include "utils.h"[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#include <flutter_windows.h>[m[41m[m
[32m+[m[32m#include <io.h>[m[41m[m
[32m+[m[32m#include <stdio.h>[m[41m[m
[32m+[m[32m#include <windows.h>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#include <iostream>[m[41m[m
[32m+[m[41m[m
[32m+[m[32mvoid CreateAndAttachConsole() {[m[41m[m
[32m+[m[32m  if (::AllocConsole()) {[m[41m[m
[32m+[m[32m    FILE *unused;[m[41m[m
[32m+[m[32m    if (freopen_s(&unused, "CONOUT$", "w", stdout)) {[m[41m[m
[32m+[m[32m      _dup2(_fileno(stdout), 1);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    if (freopen_s(&unused, "CONOUT$", "w", stderr)) {[m[41m[m
[32m+[m[32m      _dup2(_fileno(stdout), 2);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    std::ios::sync_with_stdio();[m[41m[m
[32m+[m[32m    FlutterDesktopResyncOutputStreams();[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mstd::vector<std::string> GetCommandLineArguments() {[m[41m[m
[32m+[m[32m  // Convert the UTF-16 command line arguments to UTF-8 for the Engine to use.[m[41m[m
[32m+[m[32m  int argc;[m[41m[m
[32m+[m[32m  wchar_t** argv = ::CommandLineToArgvW(::GetCommandLineW(), &argc);[m[41m[m
[32m+[m[32m  if (argv == nullptr) {[m[41m[m
[32m+[m[32m    return std::vector<std::string>();[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  std::vector<std::string> command_line_arguments;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Skip the first argument as it's the binary name.[m[41m[m
[32m+[m[32m  for (int i = 1; i < argc; i++) {[m[41m[m
[32m+[m[32m    command_line_arguments.push_back(Utf8FromUtf16(argv[i]));[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  ::LocalFree(argv);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  return command_line_arguments;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mstd::string Utf8FromUtf16(const wchar_t* utf16_string) {[m[41m[m
[32m+[m[32m  if (utf16_string == nullptr) {[m[41m[m
[32m+[m[32m    return std::string();[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m  unsigned int target_length = ::WideCharToMultiByte([m[41m[m
[32m+[m[32m      CP_UTF8, WC_ERR_INVALID_CHARS, utf16_string,[m[41m[m
[32m+[m[32m      -1, nullptr, 0, nullptr, nullptr)[m[41m[m
[32m+[m[32m    -1; // remove the trailing null character[m[41m[m
[32m+[m[32m  int input_length = (int)wcslen(utf16_string);[m[41m[m
[32m+[m[32m  std::string utf8_string;[m[41m[m
[32m+[m[32m  if (target_length == 0 || target_length > utf8_string.max_size()) {[m[41m[m
[32m+[m[32m    return utf8_string;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m  utf8_string.resize(target_length);[m[41m[m
[32m+[m[32m  int converted_length = ::WideCharToMultiByte([m[41m[m
[32m+[m[32m      CP_UTF8, WC_ERR_INVALID_CHARS, utf16_string,[m[41m[m
[32m+[m[32m      input_length, utf8_string.data(), target_length, nullptr, nullptr);[m[41m[m
[32m+[m[32m  if (converted_length == 0) {[m[41m[m
[32m+[m[32m    return std::string();[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m  return utf8_string;[m[41m[m
[32m+[m[32m}[m[41m[m
