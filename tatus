[32m+[m[32m                <placeholder placeholderIdentifier="IBFirstResponder" id="dkx-z0-nzr" sceneMemberID="firstResponder"/>[m[41m[m
[32m+[m[32m            </objects>[m[41m[m
[32m+[m[32m        </scene>[m[41m[m
[32m+[m[32m    </scenes>[m[41m[m
[32m+[m[32m</document>[m[41m[m
