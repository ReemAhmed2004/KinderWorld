[32m+[m[32m  override func awakeFromNib() {[m[41m[m
[32m+[m[32m    let flutterViewController = FlutterViewController()[m[41m[m
[32m+[m[32m    let windowFrame = self.frame[m[41m[m
[32m+[m[32m    self.contentViewController = flutterViewController[m[41m[m
[32m+[m[32m    self.setFrame(windowFrame, display: true)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    RegisterGeneratedPlugins(registry: flutterViewController)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    super.awakeFromNib()[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m}[m[41m[m
