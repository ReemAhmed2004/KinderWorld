[32m+[m[32m#include "win32_window.h"[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#include <dwmapi.h>[m[41m[m
[32m+[m[32m#include <flutter_windows.h>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#include "resource.h"[m[41m[m
[32m+[m[41m[m
[32m+[m[32mnamespace {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/// Window attribute that enables dark mode window decorations.[m[41m[m
[32m+[m[32m///[m[41m[m
[32m+[m[32m/// Redefined in case the developer's machine has a Windows SDK older than[m[41m[m
[32m+[m[32m/// version 10.0.22000.0.[m[41m[m
[32m+[m[32m/// See: https://docs.microsoft.com/windows/win32/api/dwmapi/ne-dwmapi-dwmwindowattribute[m[41m[m
[32m+[m[32m#ifndef DWMWA_USE_IMMERSIVE_DARK_MODE[m[41m[m
[32m+[m[32m#define DWMWA_USE_IMMERSIVE_DARK_MODE 20[m[41m[m
[32m+[m[32m#endif[m[41m[m
[32m+[m[41m[m
[32m+[m[32mconstexpr const wchar_t kWindowClassName[] = L"FLUTTER_RUNNER_WIN32_WINDOW";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/// Registry key for app theme preference.[m[41m[m
[32m+[m[32m///[m[41m[m
[32m+[m[32m/// A value of 0 indicates apps should use dark mode. A non-zero or missing[m[41m[m
[32m+[m[32m/// value indicates apps should use light mode.[m[41m[m
[32m+[m[32mconstexpr const wchar_t kGetPreferredBrightnessRegKey[] =[m[41m[m
[32m+[m[32m  L"Software\\Microsoft\\Windows\\CurrentVersion\\Themes\\Personalize";[m[41m[m
[32m+[m[32mconstexpr const wchar_t kGetPreferredBrightnessRegValue[] = L"AppsUseLightTheme";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// The number of Win32Window objects that currently exist.[m[41m[m
[32m+[m[32mstatic int g_active_window_count = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32musing EnableNonClientDpiScaling = BOOL __stdcall(HWND hwnd);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Scale helper to convert logical scaler values to physical using passed in[m[41m[m
[32m+[m[32m// scale factor[m[41m[m
[32m+[m[32mint Scale(int source, double scale_factor) {[m[41m[m
[32m+[m[32m  return static_cast<int>(source * scale_factor);[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Dynamically loads the |EnableNonClientDpiScaling| from the User32 module.[m[41m[m
[32m+[m[32m// This API is only needed for PerMonitor V1 awareness mode.[m[41m[m
[32m+[m[32mvoid EnableFullDpiSupportIfAvailable(HWND hwnd) {[m[41m[m
[32m+[m[32m  HMODULE user32_module = LoadLibraryA("User32.dll");[m[41m[m
[32m+[m[32m  if (!user32_module) {[m[41m[m
[32m+[m[32m    return;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m  auto enable_non_client_dpi_scaling =[m[41m[m
[32m+[m[32m      reinterpret_cast<EnableNonClientDpiScaling*>([m[41m[m
[32m+[m[32m          GetProcAddress(user32_module, "EnableNonClientDpiScaling"));[m[41m[m
[32m+[m[32m  if (enable_non_client_dpi_scaling != nullptr) {[m[41m[m
[32m+[m[32m    enable_non_client_dpi_scaling(hwnd);[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m  FreeLibrary(user32_module);[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m}  // namespace[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Manages the Win32Window's window class registration.[m[41m[m
[32m+[m[32mclass WindowClassRegistrar {[m[41m[m
[32m+[m[32m public:[m[41m[m
[32m+[m[32m  ~WindowClassRegistrar() = default;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Returns the singleton registrar instance.[m[41m[m
[32m+[m[32m  static WindowClassRegistrar* GetInstance() {[m[41m[m
[32m+[m[32m    if (!instance_) {[m[41m[m
[32m+[m[32m      instance_ = new WindowClassRegistrar();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    return instance_;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Returns the name of the window class, registering the class if it hasn't[m[41m[m
[32m+[m[32m  // previously been registered.[m[41m[m
[32m+[m[32m  const wchar_t* GetWindowClass();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Unregisters the window class. Should only be called if there are no[m[41m[m
[32m+[m[32m  // instances of the window.[m[41m[m
[32m+[m[32m  void UnregisterWindowClass();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m private:[m[41m[m
[32m+[m[32m  WindowClassRegistrar() = default;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  static WindowClassRegistrar* instance_;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  bool class_registered_ = false;[m[41m[m
[32m+[m[32m};[m[41m[m
[32m+[m[41m[m
[32m+[m[32mWindowClassRegistrar* WindowClassRegistrar::instance_ = nullptr;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mconst wchar_t* WindowClassRegistrar::GetWindowClass() {[m[41m[m
[32m+[m[32m  if (!class_registered_) {[m[41m[m
[32m+[m[32m    WNDCLASS window_class{};[m[41m[m
[32m+[m[32m    window_class.hCursor = LoadCursor(nullptr, IDC_ARROW);[m[41m[m
[32m+[m[32m    window_class.lpszClassName = kWindowClassName;[m[41m[m
[32m+[m[32m    window_class.style = CS_HREDRAW | CS_VREDRAW;[m[41m[m
[32m+[m[32m    window_class.cbClsExtra = 0;[m[41m[m
[32m+[m[32m    window_class.cbWndExtra = 0;[m[41m[m
[32m+[m[32m    window_class.hInstance = GetModuleHandle(nullptr);[m[41m[m
[32m+[m[32m    window_class.hIcon =[m[41m[m
[32m+[m[32m        LoadIcon(window_class.hInstance, MAKEINTRESOURCE(IDI_APP_ICON));[m[41m[m
[32m+[m[32m    window_class.hbrBackground = 0;[m[41m[m
[32m+[m[32m    window_class.lpszMenuName = nullptr;[m[41m[m
[32m+[m[32m    window_class.lpfnWndProc = Win32Window::WndProc;[m[41m[m
[32m+[m[32m    RegisterClass(&window_class);[m[41m[m
[32m+[m[32m    class_registered_ = true;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m  return kWindowClassName;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mvoid WindowClassRegistrar::UnregisterWindowClass() {[m[41m[m
[32m+[m[32m  UnregisterClass(kWindowClassName, nullptr);[m[41m[m
[32m+[m[32m  class_registered_ = false;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mWin32Window::Win32Window() {[m[41m[m
[32m+[m[32m  ++g_active_window_count;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mWin32Window::~Win32Window() {[m[41m[m
[32m+[m[32m  --g_active_window_count;[m[41m[m
[32m+[m[32m  Destroy();[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mbool Win32Window::Create(const std::wstring& title,[m[41m[m
[32m+[m[32m                         const Point& origin,[m[41m[m
[32m+[m[32m                         const Size& size) {[m[41m[m
[32m+[m[32m  Destroy();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  const wchar_t* window_class =[m[41m[m
[32m+[m[32m      WindowClassRegistrar::GetInstance()->GetWindowClass();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  const POINT target_point = {static_cast<LONG>(origin.x),[m[41m[m
[32m+[m[32m                              static_cast<LONG>(origin.y)};[m[41m[m
[32m+[m[32m  HMONITOR monitor = MonitorFromPoint(target_point, MONITOR_DEFAULTTONEAREST);[m[41m[m
[32m+[m[32m  UINT dpi = FlutterDesktopGetDpiForMonitor(monitor);[m[41m[m
[32m+[m[32m  double scale_factor = dpi / 96.0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  HWND window = CreateWindow([m[41m[m
[32m+[m[32m      window_class, title.c_str(), WS_OVERLAPPEDWINDOW,[m[41m[m
[32m+[m[32m      Scale(origin.x, scale_factor), Scale(origin.y, scale_factor),[m[41m[m
[32m+[m[32m      Scale(size.width, scale_factor), Scale(size.height, scale_factor),[m[41m[m
[32m+[m[32m      nullptr, nullptr, GetModuleHandle(nullptr), this);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  if (!window) {[m[41m[m
[32m+[m[32m    return false;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  UpdateTheme(window);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  return OnCreate();[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mbool Win32Window::Show() {[m[41m[m
[32m+[m[32m  return ShowWindow(window_handle_, SW_SHOWNORMAL);[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// static[m[41m[m
[32m+[m[32mLRESULT CALLBACK Win32Window::WndProc(HWND const window,[m[41m[m
[32m+[m[32m                                      UINT const message,[m[41m[m
[32m+[m[32m                                      WPARAM const wparam,[m[41m[m
[32m+[m[32m                                      LPARAM const lparam) noexcept {[m[41m[m
[32m+[m[32m  if (message == WM_NCCREATE) {[m[41m[m
[32m+[m[32m    auto window_struct = reinterpret_cast<CREATESTRUCT*>(lparam);[m[41m[m
[32m+[m[32m    SetWindowLongPtr(window, GWLP_USERDATA,[m[41m[m
[32m+[m[32m                     reinterpret_cast<LONG_PTR>(window_struct->lpCreateParams));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    auto that = static_cast<Win32Window*>(window_struct->lpCreateParams);[m[41m[m
[32m+[m[32m    EnableFullDpiSupportIfAvailable(window);[m[41m[m
[32m+[m[32m    that->window_handle_ = window;[m[41m[m
[32m+[m[32m  } else if (Win32Window* that = GetThisFromHandle(window)) {[m[41m[m
[32m+[m[32m    return that->MessageHandler(window, message, wparam, lparam);[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  return DefWindowProc(window, message, wparam, lparam);[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mLRESULT[m[41m[m
[32m+[m[32mWin32Window::MessageHandler(HWND hwnd,[m[41m[m
[32m+[m[32m                            UINT const message,[m[41m[m
[32m+[m[32m                            WPARAM const wparam,[m[41m[m
[32m+[m[32m                            LPARAM const lparam) noexcept {[m[41m[m
[32m+[m[32m  switch (message) {[m[41m[m
[32m+[m[32m    case WM_DESTROY:[m[41m[m
[32m+[m[32m      window_handle_ = nullptr;[m[41m[m
[32m+[m[32m      Destroy();[m[41m[m
[32m+[m[32m      if (quit_on_close_) {[m[41m[m
[32m+[m[32m        PostQuitMessage(0);[m[41m[m
[32m+[m[32m      }[m[41m[m
[32m+[m[32m      return 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    case WM_DPICHANGED: {[m[41m[m
[32m+[m[32m      auto newRectSize = reinterpret_cast<RECT*>(lparam);[m[41m[m
[32m+[m[32m      LONG newWidth = newRectSize->right - newRectSize->left;[m[41m[m
[32m+[m[32m      LONG newHeight = newRectSize->bottom - newRectSize->top;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      SetWindowPos(hwnd, nullptr, newRectSize->left, newRectSize->top, newWidth,[m[41m[m
[32m+[m[32m                   newHeight, SWP_NOZORDER | SWP_NOACTIVATE);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      return 0;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    case WM_SIZE: {[m[41m[m
[32m+[m[32m      RECT rect = GetClientArea();[m[41m[m
[32m+[m[32m      if (child_content_ != nullptr) {[m[41m[m
[32m+[m[32m        // Size and position the child window.[m[41m[m
[32m+[m[32m        MoveWindow(child_content_, rect.left, rect.top, rect.right - rect.left,[m[41m[m
[32m+[m[32m                   rect.bottom - rect.top, TRUE);[m[41m[m
[32m+[m[32m      }[m[41m[m
[32m+[m[32m      return 0;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    case WM_ACTIVATE:[m[41m[m
[32m+[m[32m      if (child_content_ != nullptr) {[m[41m[m
[32m+[m[32m        SetFocus(child_content_);[m[41m[m
[32m+[m[32m      }[m[41m[m
[32m+[m[32m      return 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    case WM_DWMCOLORIZATIONCOLORCHANGED:[m[41m[m
[32m+[m[32m      UpdateTheme(hwnd);[m[41m[m
[32m+[m[32m      return 0;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  return DefWindowProc(window_handle_, message, wparam, lparam);[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mvoid Win32Window::Destroy() {[m[41m[m
[32m+[m[32m  OnDestroy();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  if (window_handle_) {[m[41m[m
[32m+[m[32m    DestroyWindow(window_handle_);[m[41m[m
[32m+[m[32m    window_handle_ = nullptr;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m  if (g_active_window_count == 0) {[m[41m[m
[32m+[m[32m    WindowClassRegistrar::GetInstance()->UnregisterWindowClass();[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mWin32Window* Win32Window::GetThisFromHandle(HWND const window) noexcept {[m[41m[m
[32m+[m[32m  return reinterpret_cast<Win32Window*>([m[41m[m
[32m+[m[32m      GetWindowLongPtr(window, GWLP_USERDATA));[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mvoid Win32Window::SetChildContent(HWND content) {[m[41m[m
[32m+[m[32m  child_content_ = content;[m[41m[m
[32m+[m[32m  SetParent(content, window_handle_);[m[41m[m
[32m+[m[32m  RECT frame = GetClientArea();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  MoveWindow(content, frame.left, frame.top, frame.right - frame.left,[m[41m[m
[32m+[m[32m             frame.bottom - frame.top, true);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  SetFocus(child_content_);[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mRECT Win32Window::GetClientArea() {[m[41m[m
[32m+[m[32m  RECT frame;[m[41m[m
[32m+[m[32m  GetClientRect(window_handle_, &frame);[m[41m[m
[32m+[m[32m  return frame;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mHWND Win32Window::GetHandle() {[m[41m[m
[32m+[m[32m  return window_handle_;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mvoid Win32Window::SetQuitOnClose(bool quit_on_close) {[m[41m[m
[32m+[m[32m  quit_on_close_ = quit_on_close;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mbool Win32Window::OnCreate() {[m[41m[m
[32m+[m[32m  // No-op; provided for subclasses.[m[41m[m
[32m+[m[32m  return true;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mvoid Win32Window::OnDestroy() {[m[41m[m
[32m+[m[32m  // No-op; provided for subclasses.[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mvoid Win32Window::UpdateTheme(HWND const window) {[m[41m[m
[32m+[m[32m  DWORD light_mode;[m[41m[m
[32m+[m[32m  DWORD light_mode_size = sizeof(light_mode);[m[41m[m
[32m+[m[32m  LSTATUS result = RegGetValue(HKEY_CURRENT_USER, kGetPreferredBrightnessRegKey,[m[41m[m
[32m+[m[32m                               kGetPreferredBrightnessRegValue,[m[41m[m
[32m+[m[32m                               RRF_RT_REG_DWORD, nullptr, &light_mode,[m[41m[m
[32m+[m[32m                               &light_mode_size);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  if (result == ERROR_SUCCESS) {[m[41m[m
[32m+[m[32m    BOOL enable_dark_mode = light_mode == 0;[m[41m[m
[32m+[m[32m    DwmSetWindowAttribute(window, DWMWA_USE_IMMERSIVE_DARK_MODE,[m[41m[m
[32m+[m[32m                          &enable_dark_mode, sizeof(enable_dark_mode));[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m}[m[41m[m
[1mdiff --git a/windows/runner/win32_window.h b/windows/runner/win32_window.h[m
[1mnew file mode 100644[m
[1mindex 0000000..49b847f[m
[1m--- /dev/null[m
[1m+++ b/windows/runner/win32_window.h[m
[36m@@ -0,0 +1,102 @@[m
[32m+[m[32m#ifndef RUNNER_WIN32_WINDOW_H_[m[41m[m
[32m+[m[32m#define RUNNER_WIN32_WINDOW_H_[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#include <windows.h>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#include <functional>[m[41m[m
[32m+[m[32m#include <memory>[m[41m[m
[32m+[m[32m#include <string>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// A class abstraction for a high DPI-aware Win32 Window. Intended to be[m[41m[m
[32m+[m[32m// inherited from by classes that wish to specialize with custom[m[41m[m
[32m+[m[32m// rendering and input handling[m[41m[m
[32m+[m[32mclass Win32Window {[m[41m[m
[32m+[m[32m public:[m[41m[m
[32m+[m[32m  struct Point {[m[41m[m
[32m+[m[32m    unsigned int x;[m[41m[m
[32m+[m[32m    unsigned int y;[m[41m[m
[32m+[m[32m    Point(unsigned int x, unsigned int y) : x(x), y(y) {}[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  struct Size {[m[41m[m
[32m+[m[32m    unsigned int width;[m[41m[m
[32m+[m[32m    unsigned int height;[m[41m[m
[32m+[m[32m    Size(unsigned int width, unsigned int height)[m[41m[m
[32m+[m[32m        : width(width), height(height) {}[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  Win32Window();[m[41m[m
[32m+[m[32m  virtual ~Win32Window();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Creates a win32 window with |title| that is positioned and sized using[m[41m[m
[32m+[m[32m  // |origin| and |size|. New windows are created on the default monitor. Window[m[41m[m
[32m+[m[32m  // sizes are specified to the OS in physical pixels, hence to ensure a[m[41m[m
[32m+[m[32m  // consistent size this function will scale the inputted width and height as[m[41m[m
[32m+[m[32m  // as appropriate for the default monitor. The window is invisible until[m[41m[m
[32m+[m[32m  // |Show| is called. Returns true if the window was created successfully.[m[41m[m
[32m+[m[32m  bool Create(const std::wstring& title, const Point& origin, const Size& size);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Show the current window. Returns true if the window was successfully shown.[m[41m[m
[32m+[m[32m  bool Show();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Release OS resources associated with window.[m[41m[m
[32m+[m[32m  void Destroy();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Inserts |content| into the window tree.[m[41m[m
[32m+[m[32m  void SetChildContent(HWND content);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Returns the backing Window handle to enable clients to set icon and other[m[41m[m
[32m+[m[32m  // window properties. Returns nullptr if the window has been destroyed.[m[41m[m
[32m+[m[32m  HWND GetHandle();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // If true, closing this window will quit the application.[m[41m[m
[32m+[m[32m  void SetQuitOnClose(bool quit_on_close);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Return a RECT representing the bounds of the current client area.[m[41m[m
[32m+[m[32m  RECT GetClientArea();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m protected:[m[41m[m
[32m+[m[32m  // Processes and route salient window messages for mouse handling,[m[41m[m
[32m+[m[32m  // size change and DPI. Delegates handling of these to member overloads that[m[41m[m
[32m+[m[32m  // inheriting classes can handle.[m[41m[m
[32m+[m[32m  virtual LRESULT MessageHandler(HWND window,[m[41m[m
[32m+[m[32m                                 UINT const message,[m[41m[m
[32m+[m[32m                                 WPARAM const wparam,[m[41m[m
[32m+[m[32m                                 LPARAM const lparam) noexcept;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Called when CreateAndShow is called, allowing subclass window-related[m[41m[m
[32m+[m[32m  // setup. Subclasses should return false if setup fails.[m[41m[m
[32m+[m[32m  virtual bool OnCreate();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Called when Destroy is called.[m[41m[m
[32m+[m[32m  virtual void OnDestroy();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m private:[m[41m[m
[32m+[m[32m  friend class WindowClassRegistrar;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // OS callback called by message pump. Handles the WM_NCCREATE message which[m[41m[m
[32m+[m[32m  // is passed when the non-client area is being created and enables automatic[m[41m[m
[32m+[m[32m  // non-client DPI scaling so that the non-client area automatically[m[41m[m
[32m+[m[32m  // responds to changes in DPI. All other messages are handled by[m[41m[m
[32m+[m[32m  // MessageHandler.[m[41m[m
[32m+[m[32m  static LRESULT CALLBACK WndProc(HWND const window,[m[41m[m
[32m+[m[32m                                  UINT const message,[m[41m[m
[32m+[m[32m                                  WPARAM const wparam,[m[41m[m
[32m+[m[32m                                  LPARAM const lparam) noexcept;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Retrieves a class instance pointer for |window|[m[41m[m
[32m+[m[32m  static Win32Window* GetThisFromHandle(HWND const window) noexcept;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Update the window frame's theme to match the system theme.[m[41m[m
[32m+[m[32m  static void UpdateTheme(HWND const window);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  bool quit_on_close_ = false;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // window handle for top level window.[m[41m[m
[32m+[m[32m  HWND window_handle_ = nullptr;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // window handle for hosted content.[m[41m[m
[32m+[m[32m  HWND child_content_ = nullptr;[m[41m[m
[32m+[m[32m};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#endif  // RUNNER_WIN32_WINDOW_H_[m[41m[m
