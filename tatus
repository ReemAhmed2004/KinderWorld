[36m@@ -0,0 +1,19 @@[m
[32m+[m[32m#ifndef RUNNER_UTILS_H_[m[41m[m
[32m+[m[32m#define RUNNER_UTILS_H_[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#include <string>[m[41m[m
[32m+[m[32m#include <vector>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Creates a console for the process, and redirects stdout and stderr to[m[41m[m
[32m+[m[32m// it for both the runner and the Flutter library.[m[41m[m
[32m+[m[32mvoid CreateAndAttachConsole();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Takes a null-terminated wchar_t* encoded in UTF-16 and returns a std::string[m[41m[m
[32m+[m[32m// encoded in UTF-8. Returns an empty std::string on failure.[m[41m[m
[32m+[m[32mstd::string Utf8FromUtf16(const wchar_t* utf16_string);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Gets the command line arguments passed in as a std::vector<std::string>,[m[41m[m
[32m+[m[32m// encoded in UTF-8. Returns an empty std::vector<std::string> on failure.[m[41m[m
[32m+[m[32mstd::vector<std::string> GetCommandLineArguments();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#endif  // RUNNER_UTILS_H_[m[41m[m
