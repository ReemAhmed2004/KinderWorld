[32m+[m[32m# Project-level configuration.[m[41m[m
[32m+[m[32mcmake_minimum_required(VERSION 3.14)[m[41m[m
[32m+[m[32mproject(kinder_world LANGUAGES CXX)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# The name of the executable created for the application. Change this to change[m[41m[m
[32m+[m[32m# the on-disk name of your application.[m[41m[m
[32m+[m[32mset(BINARY_NAME "kinder_world")[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Explicitly opt in to modern CMake behaviors to avoid warnings with recent[m[41m[m
[32m+[m[32m# versions of CMake.[m[41m[m
[32m+[m[32mcmake_policy(VERSION 3.14...3.25)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Define build configuration option.[m[41m[m
[32m+[m[32mget_property(IS_MULTICONFIG GLOBAL PROPERTY GENERATOR_IS_MULTI_CONFIG)[m[41m[m
[32m+[m[32mif(IS_MULTICONFIG)[m[41m[m
[32m+[m[32m  set(CMAKE_CONFIGURATION_TYPES "Debug;Profile;Release"[m[41m[m
[32m+[m[32m    CACHE STRING "" FORCE)[m[41m[m
[32m+[m[32melse()[m[41m[m
[32m+[m[32m  if(NOT CMAKE_BUILD_TYPE AND NOT CMAKE_CONFIGURATION_TYPES)[m[41m[m
[32m+[m[32m    set(CMAKE_BUILD_TYPE "Debug" CACHE[m[41m[m
[32m+[m[32m      STRING "Flutter build mode" FORCE)[m[41m[m
[32m+[m[32m    set_property(CACHE CMAKE_BUILD_TYPE PROPERTY STRINGS[m[41m[m
[32m+[m[32m      "Debug" "Profile" "Release")[m[41m[m
[32m+[m[32m  endif()[m[41m[m
[32m+[m[32mendif()[m[41m[m
[32m+[m[32m# Define settings for the Profile build mode.[m[41m[m
[32m+[m[32mset(CMAKE_EXE_LINKER_FLAGS_PROFILE "${CMAKE_EXE_LINKER_FLAGS_RELEASE}")[m[41m[m
[32m+[m[32mset(CMAKE_SHARED_LINKER_FLAGS_PROFILE "${CMAKE_SHARED_LINKER_FLAGS_RELEASE}")[m[41m[m
[32m+[m[32mset(CMAKE_C_FLAGS_PROFILE "${CMAKE_C_FLAGS_RELEASE}")[m[41m[m
[32m+[m[32mset(CMAKE_CXX_FLAGS_PROFILE "${CMAKE_CXX_FLAGS_RELEASE}")[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Use Unicode for all projects.[m[41m[m
[32m+[m[32madd_definitions(-DUNICODE -D_UNICODE)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Compilation settings that should be applied to most targets.[m[41m[m
[32m+[m[32m#[m[41m[m
[32m+[m[32m# Be cautious about adding new options here, as plugins use this function by[m[41m[m
[32m+[m[32m# default. In most cases, you should add new options to specific targets instead[m[41m[m
[32m+[m[32m# of modifying this function.[m[41m[m
[32m+[m[32mfunction(APPLY_STANDARD_SETTINGS TARGET)[m[41m[m
[32m+[m[32m  target_compile_features(${TARGET} PUBLIC cxx_std_17)[m[41m[m
[32m+[m[32m  target_compile_options(${TARGET} PRIVATE /W4 /WX /wd"4100")[m[41m[m
[32m+[m[32m  target_compile_options(${TARGET} PRIVATE /EHsc)[m[41m[m
[32m+[m[32m  target_compile_definitions(${TARGET} PRIVATE "_HAS_EXCEPTIONS=0")[m[41m[m
[32m+[m[32m  target_compile_definitions(${TARGET} PRIVATE "$<$<CONFIG:Debug>:_DEBUG>")[m[41m[m
[32m+[m[32mendfunction()[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Flutter library and tool build rules.[m[41m[m
[32m+[m[32mset(FLUTTER_MANAGED_DIR "${CMAKE_CURRENT_SOURCE_DIR}/flutter")[m[41m[m
[32m+[m[32madd_subdirectory(${FLUTTER_MANAGED_DIR})[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Application build; see runner/CMakeLists.txt.[m[41m[m
[32m+[m[32madd_subdirectory("runner")[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Generated plugin build rules, which manage building the plugins and adding[m[41m[m
[32m+[m[32m# them to the application.[m[41m[m
[32m+[m[32minclude(flutter/generated_plugins.cmake)[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# === Installation ===[m[41m[m
[32m+[m[32m# Support files are copied into place next to the executable, so that it can[m[41m[m
[32m+[m[32m# run in place. This is done instead of making a separate bundle (as on Linux)[m[41m[m
[32m+[m[32m# so that building and running from within Visual Studio will work.[m[41m[m
[32m+[m[32mset(BUILD_BUNDLE_DIR "$<TARGET_FILE_DIR:${BINARY_NAME}>")[m[41m[m
[32m+[m[32m# Make the "install" step default, as it's required to run.[m[41m[m
[32m+[m[32mset(CMAKE_VS_INCLUDE_INSTALL_TO_DEFAULT_BUILD 1)[m[41m[m
[32m+[m[32mif(CMAKE_INSTALL_PREFIX_INITIALIZED_TO_DEFAULT)[m[41m[m
[32m+[m[32m  set(CMAKE_INSTALL_PREFIX "${BUILD_BUNDLE_DIR}" CACHE PATH "..." FORCE)[m[41m[m
[32m+[m[32mendif()[m[41m[m
[32m+[m[41m[m
[32m+[m[32mset(INSTALL_BUNDLE_DATA_DIR "${CMAKE_INSTALL_PREFIX}/data")[m[41m[m
[32m+[m[32mset(INSTALL_BUNDLE_LIB_DIR "${CMAKE_INSTALL_PREFIX}")[m[41m[m
[32m+[m[41m[m
[32m+[m[32minstall(TARGETS ${BINARY_NAME} RUNTIME DESTINATION "${CMAKE_INSTALL_PREFIX}"[m[41m[m
[32m+[m[32m  COMPONENT Runtime)[m[41m[m
[32m+[m[41m[m
[32m+[m[32minstall(FILES "${FLUTTER_ICU_DATA_FILE}" DESTINATION "${INSTALL_BUNDLE_DATA_DIR}"[m[41m[m
[32m+[m[32m  COMPONENT Runtime)[m[41m[m
[32m+[m[41m[m
[32m+[m[32minstall(FILES "${FLUTTER_LIBRARY}" DESTINATION "${INSTALL_BUNDLE_LIB_DIR}"[m[41m[m
[32m+[m[32m  COMPONENT Runtime)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mif(PLUGIN_BUNDLED_LIBRARIES)[m[41m[m
[32m+[m[32m  install(FILES "${PLUGIN_BUNDLED_LIBRARIES}"[m[41m[m
[32m+[m[32m    DESTINATION "${INSTALL_BUNDLE_LIB_DIR}"[m[41m[m
[32m+[m[32m    COMPONENT Runtime)[m[41m[m
[32m+[m[32mendif()[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Copy the native assets provided by the build.dart from all packages.[m[41m[m
[32m+[m[32mset(NATIVE_ASSETS_DIR "${PROJECT_BUILD_DIR}native_assets/windows/")[m[41m[m
[32m+[m[32minstall(DIRECTORY "${NATIVE_ASSETS_DIR}"[m[41m[m
[32m+[m[32m   DESTINATION "${INSTALL_BUNDLE_LIB_DIR}"[m[41m[m
[32m+[m[32m   COMPONENT Runtime)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Fully re-copy the assets directory on each build to avoid having stale files[m[41m[m
[32m+[m[32m# from a previous install.[m[41m[m
[32m+[m[32mset(FLUTTER_ASSET_DIR_NAME "flutter_assets")[m[41m[m
[32m+[m[32minstall(CODE "[m[41m[m
[32m+[m[32m  file(REMOVE_RECURSE \"${INSTALL_BUNDLE_DATA_DIR}/${FLUTTER_ASSET_DIR_NAME}\")[m[41m[m
[32m+[m[32m  " COMPONENT Runtime)[m[41m[m
[32m+[m[32minstall(DIRECTORY "${PROJECT_BUILD_DIR}/${FLUTTER_ASSET_DIR_NAME}"[m[41m[m
[32m+[m[32m  DESTINATION "${INSTALL_BUNDLE_DATA_DIR}" COMPONENT Runtime)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Install the AOT library on non-Debug builds only.[m[41m[m
[32m+[m[32minstall(FILES "${AOT_LIBRARY}" DESTINATION "${INSTALL_BUNDLE_DATA_DIR}"[m[41m[m
[32m+[m[32m  CONFIGURATIONS Profile;Release[m[41m[m
[32m+[m[32m  COMPONENT Runtime)[m[41m[m
[1mdiff --git a/windows/flutter/CMakeLists.txt b/windows/flutter/CMakeLists.txt[m
[1mnew file mode 100644[m
[1mindex 0000000..efb62eb[m
[1m--- /dev/null[m
[1m+++ b/windows/flutter/CMakeLists.txt[m
[36m@@ -0,0 +1,109 @@[m
[32m+[m[32m# This file controls Flutter-level build steps. It should not be edited.[m[41m[m
[32m+[m[32mcmake_minimum_required(VERSION 3.14)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mset(EPHEMERAL_DIR "${CMAKE_CURRENT_SOURCE_DIR}/ephemeral")[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Configuration provided via flutter tool.[m[41m[m
[32m+[m[32minclude(${EPHEMERAL_DIR}/generated_config.cmake)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# TODO: Move the rest of this into files in ephemeral. See[m[41m[m
[32m+[m[32m# https://github.com/flutter/flutter/issues/57146.[m[41m[m
[32m+[m[32mset(WRAPPER_ROOT "${EPHEMERAL_DIR}/cpp_client_wrapper")[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Set fallback configurations for older versions of the flutter tool.[m[41m[m
[32m+[m[32mif (NOT DEFINED FLUTTER_TARGET_PLATFORM)[m[41m[m
[32m+[m[32m  set(FLUTTER_TARGET_PLATFORM "windows-x64")[m[41m[m
[32m+[m[32mendif()[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# === Flutter Library ===[m[41m[m
[32m+[m[32mset(FLUTTER_LIBRARY "${EPHEMERAL_DIR}/flutter_windows.dll")[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Published to parent scope for install step.[m[41m[m
[32m+[m[32mset(FLUTTER_LIBRARY ${FLUTTER_LIBRARY} PARENT_SCOPE)[m[41m[m
[32m+[m[32mset(FLUTTER_ICU_DATA_FILE "${EPHEMERAL_DIR}/icudtl.dat" PARENT_SCOPE)[m[41m[m
[32m+[m[32mset(PROJECT_BUILD_DIR "${PROJECT_DIR}/build/" PARENT_SCOPE)[m[41m[m
[32m+[m[32mset(AOT_LIBRARY "${PROJECT_DIR}/build/windows/app.so" PARENT_SCOPE)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mlist(APPEND FLUTTER_LIBRARY_HEADERS[m[41m[m
[32m+[m[32m  "flutter_export.h"[m[41m[m
[32m+[m[32m  "flutter_windows.h"[m[41m[m
[32m+[m[32m  "flutter_messenger.h"[m[41m[m
[32m+[m[32m  "flutter_plugin_registrar.h"[m[41m[m
[32m+[m[32m  "flutter_texture_registrar.h"[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32mlist(TRANSFORM FLUTTER_LIBRARY_HEADERS PREPEND "${EPHEMERAL_DIR}/")[m[41m[m
[32m+[m[32madd_library(flutter INTERFACE)[m[41m[m
[32m+[m[32mtarget_include_directories(flutter INTERFACE[m[41m[m
[32m+[m[32m  "${EPHEMERAL_DIR}"[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32mtarget_link_libraries(flutter INTERFACE "${FLUTTER_LIBRARY}.lib")[m[41m[m
[32m+[m[32madd_dependencies(flutter flutter_assemble)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# === Wrapper ===[m[41m[m
[32m+[m[32mlist(APPEND CPP_WRAPPER_SOURCES_CORE[m[41m[m
[32m+[m[32m  "core_implementations.cc"[m[41m[m
[32m+[m[32m  "standard_codec.cc"[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32mlist(TRANSFORM CPP_WRAPPER_SOURCES_CORE PREPEND "${WRAPPER_ROOT}/")[m[41m[m
[32m+[m[32mlist(APPEND CPP_WRAPPER_SOURCES_PLUGIN[m[41m[m
[32m+[m[32m  "plugin_registrar.cc"[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32mlist(TRANSFORM CPP_WRAPPER_SOURCES_PLUGIN PREPEND "${WRAPPER_ROOT}/")[m[41m[m
[32m+[m[32mlist(APPEND CPP_WRAPPER_SOURCES_APP[m[41m[m
[32m+[m[32m  "flutter_engine.cc"[m[41m[m
[32m+[m[32m  "flutter_view_controller.cc"[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32mlist(TRANSFORM CPP_WRAPPER_SOURCES_APP PREPEND "${WRAPPER_ROOT}/")[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Wrapper sources needed for a plugin.[m[41m[m
[32m+[m[32madd_library(flutter_wrapper_plugin STATIC[m[41m[m
[32m+[m[32m  ${CPP_WRAPPER_SOURCES_CORE}[m[41m[m
[32m+[m[32m  ${CPP_WRAPPER_SOURCES_PLUGIN}[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32mapply_standard_settings(flutter_wrapper_plugin)[m[41m[m
[32m+[m[32mset_target_properties(flutter_wrapper_plugin PROPERTIES[m[41m[m
[32m+[m[32m  POSITION_INDEPENDENT_CODE ON)[m[41m[m
[32m+[m[32mset_target_properties(flutter_wrapper_plugin PROPERTIES[m[41m[m
[32m+[m[32m  CXX_VISIBILITY_PRESET hidden)[m[41m[m
[32m+[m[32mtarget_link_libraries(flutter_wrapper_plugin PUBLIC flutter)[m[41m[m
[32m+[m[32mtarget_include_directories(flutter_wrapper_plugin PUBLIC[m[41m[m
[32m+[m[32m  "${WRAPPER_ROOT}/include"[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32madd_dependencies(flutter_wrapper_plugin flutter_assemble)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Wrapper sources needed for the runner.[m[41m[m
[32m+[m[32madd_library(flutter_wrapper_app STATIC[m[41m[m
[32m+[m[32m  ${CPP_WRAPPER_SOURCES_CORE}[m[41m[m
[32m+[m[32m  ${CPP_WRAPPER_SOURCES_APP}[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32mapply_standard_settings(flutter_wrapper_app)[m[41m[m
[32m+[m[32mtarget_link_libraries(flutter_wrapper_app PUBLIC flutter)[m[41m[m
[32m+[m[32mtarget_include_directories(flutter_wrapper_app PUBLIC[m[41m[m
[32m+[m[32m  "${WRAPPER_ROOT}/include"[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32madd_dependencies(flutter_wrapper_app flutter_assemble)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# === Flutter tool backend ===[m[41m[m
[32m+[m[32m# _phony_ is a non-existent file to force this command to run every time,[m[41m[m
[32m+[m[32m# since currently there's no way to get a full input/output list from the[m[41m[m
[32m+[m[32m# flutter tool.[m[41m[m
[32m+[m[32mset(PHONY_OUTPUT "${CMAKE_CURRENT_BINARY_DIR}/_phony_")[m[41m[m
[32m+[m[32mset_source_files_properties("${PHONY_OUTPUT}" PROPERTIES SYMBOLIC TRUE)[m[41m[m
[32m+[m[32madd_custom_command([m[41m[m
[32m+[m[32m  OUTPUT ${FLUTTER_LIBRARY} ${FLUTTER_LIBRARY_HEADERS}[m[41m[m
[32m+[m[32m    ${CPP_WRAPPER_SOURCES_CORE} ${CPP_WRAPPER_SOURCES_PLUGIN}[m[41m[m
[32m+[m[32m    ${CPP_WRAPPER_SOURCES_APP}[m[41m[m
[32m+[m[32m    ${PHONY_OUTPUT}[m[41m[m
[32m+[m[32m  COMMAND ${CMAKE_COMMAND} -E env[m[41m[m
[32m+[m[32m    ${FLUTTER_TOOL_ENVIRONMENT}[m[41m[m
[32m+[m[32m    "${FLUTTER_ROOT}/packages/flutter_tools/bin/tool_backend.bat"[m[41m[m
[32m+[m[32m      ${FLUTTER_TARGET_PLATFORM} $<CONFIG>[m[41m[m
[32m+[m[32m  VERBATIM[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32madd_custom_target(flutter_assemble DEPENDS[m[41m[m
[32m+[m[32m  "${FLUTTER_LIBRARY}"[m[41m[m
[32m+[m[32m  ${FLUTTER_LIBRARY_HEADERS}[m[41m[m
[32m+[m[32m  ${CPP_WRAPPER_SOURCES_CORE}[m[41m[m
[32m+[m[32m  ${CPP_WRAPPER_SOURCES_PLUGIN}[m[41m[m
[32m+[m[32m  ${CPP_WRAPPER_SOURCES_APP}[m[41m[m
[32m+[m[32m)[m[41m[m
