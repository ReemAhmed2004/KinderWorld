[32m+[m[32m#ifndef RUNNER_FLUTTER_WINDOW_H_[m[41m[m
[32m+[m[32m#define RUNNER_FLUTTER_WINDOW_H_[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#include <flutter/dart_project.h>[m[41m[m
[32m+[m[32m#include <flutter/flutter_view_controller.h>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#include <memory>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#include "win32_window.h"[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// A window that does nothing but host a Flutter view.[m[41m[m
[32m+[m[32mclass FlutterWindow : public Win32Window {[m[41m[m
[32m+[m[32m public:[m[41m[m
[32m+[m[32m  // Creates a new FlutterWindow hosting a Flutter view running |project|.[m[41m[m
[32m+[m[32m  explicit FlutterWindow(const flutter::DartProject& project);[m[41m[m
[32m+[m[32m  virtual ~FlutterWindow();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m protected:[m[41m[m
[32m+[m[32m  // Win32Window:[m[41m[m
[32m+[m[32m  bool OnCreate() override;[m[41m[m
[32m+[m[32m  void OnDestroy() override;[m[41m[m
[32m+[m[32m  LRESULT MessageHandler(HWND window, UINT const message, WPARAM const wparam,[m[41m[m
[32m+[m[32m                         LPARAM const lparam) noexcept override;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m private:[m[41m[m
[32m+[m[32m  // The project to run.[m[41m[m
[32m+[m[32m  flutter::DartProject project_;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // The Flutter instance hosted by this window.[m[41m[m
[32m+[m[32m  std::unique_ptr<flutter::FlutterViewController> flutter_controller_;[m[41m[m
[32m+[m[32m};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#endif  // RUNNER_FLUTTER_WINDOW_H_[m[41m[m
[1mdiff --git a/windows/runner/main.cpp b/windows/runner/main.cpp[m
