[32m+[m[41m[m
[32m+[m[32mFlutterWindow::FlutterWindow(const flutter::DartProject& project)[m[41m[m
[32m+[m[32m    : project_(project) {}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mFlutterWindow::~FlutterWindow() {}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mbool FlutterWindow::OnCreate() {[m[41m[m
[32m+[m[32m  if (!Win32Window::OnCreate()) {[m[41m[m
[32m+[m[32m    return false;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  RECT frame = GetClientArea();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // The size here must match the window dimensions to avoid unnecessary surface[m[41m[m
[32m+[m[32m  // creation / destruction in the startup path.[m[41m[m
[32m+[m[32m  flutter_controller_ = std::make_unique<flutter::FlutterViewController>([m[41m[m
[32m+[m[32m      frame.right - frame.left, frame.bottom - frame.top, project_);[m[41m[m
[32m+[m[32m  // Ensure that basic setup of the controller was successful.[m[41m[m
[32m+[m[32m  if (!flutter_controller_->engine() || !flutter_controller_->view()) {[m[41m[m
[32m+[m[32m    return false;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m  RegisterPlugins(flutter_controller_->engine());[m[41m[m
[32m+[m[32m  SetChildContent(flutter_controller_->view()->GetNativeWindow());[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  flutter_controller_->engine()->SetNextFrameCallback([&]() {[m[41m[m
[32m+[m[32m    this->Show();[m[41m[m
[32m+[m[32m  });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Flutter can complete the first frame before the "show window" callback is[m[41m[m
[32m+[m[32m  // registered. The following call ensures a frame is pending to ensure the[m[41m[m
[32m+[m[32m  // window is shown. It is a no-op if the first frame hasn't completed yet.[m[41m[m
[32m+[m[32m  flutter_controller_->ForceRedraw();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  return true;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mvoid FlutterWindow::OnDestroy() {[m[41m[m
[32m+[m[32m  if (flutter_controller_) {[m[41m[m
[32m+[m[32m    flutter_controller_ = nullptr;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  Win32Window::OnDestroy();[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mLRESULT[m[41m[m
[32m+[m[32mFlutterWindow::MessageHandler(HWND hwnd, UINT const message,[m[41m[m
[32m+[m[32m                              WPARAM const wparam,[m[41m[m
[32m+[m[32m                              LPARAM const lparam) noexcept {[m[41m[m
[32m+[m[32m  // Give Flutter, including plugins, an opportunity to handle window messages.[m[41m[m
[32m+[m[32m  if (flutter_controller_) {[m[41m[m
[32m+[m[32m    std::optional<LRESULT> result =[m[41m[m
[32m+[m[32m        flutter_controller_->HandleTopLevelWindowProc(hwnd, message, wparam,[m[41m[m
[32m+[m[32m                                                      lparam);[m[41m[m
[32m+[m[32m    if (result) {[m[41m[m
[32m+[m[32m      return *result;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  switch (message) {[m[41m[m
[32m+[m[32m    case WM_FONTCHANGE:[m[41m[m
[32m+[m[32m      flutter_controller_->engine()->ReloadSystemFonts();[m[41m[m
[32m+[m[32m      break;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  return Win32Window::MessageHandler(hwnd, message, wparam, lparam);[m[41m[m
[32m+[m[32m}[m[41m[m
[1mdiff --git a/windows/runner/flutter_window.h b/windows/runner/flutter_window.h[m
[1mnew file mode 100644[m
