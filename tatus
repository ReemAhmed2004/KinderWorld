[1mdiff --git a/linux/CMakeLists.txt b/linux/CMakeLists.txt[m
[1mnew file mode 100644[m
[1mindex 0000000..6c2ec7b[m
[1m--- /dev/null[m
[1m+++ b/linux/CMakeLists.txt[m
[36m@@ -0,0 +1,128 @@[m
[32m+[m[32m# Project-level configuration.[m[41m[m
[32m+[m[32mcmake_minimum_required(VERSION 3.13)[m[41m[m
[32m+[m[32mproject(runner LANGUAGES CXX)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# The name of the executable created for the application. Change this to change[m[41m[m
[32m+[m[32m# the on-disk name of your application.[m[41m[m
[32m+[m[32mset(BINARY_NAME "kinder_world")[m[41m[m
[32m+[m[32m# The unique GTK application identifier for this application. See:[m[41m[m
[32m+[m[32m# https://wiki.gnome.org/HowDoI/ChooseApplicationID[m[41m[m
[32m+[m[32mset(APPLICATION_ID "com.example.kinder_world")[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Explicitly opt in to modern CMake behaviors to avoid warnings with recent[m[41m[m
[32m+[m[32m# versions of CMake.[m[41m[m
[32m+[m[32mcmake_policy(SET CMP0063 NEW)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Load bundled libraries from the lib/ directory relative to the binary.[m[41m[m
[32m+[m[32mset(CMAKE_INSTALL_RPATH "$ORIGIN/lib")[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Root filesystem for cross-building.[m[41m[m
[32m+[m[32mif(FLUTTER_TARGET_PLATFORM_SYSROOT)[m[41m[m
[32m+[m[32m  set(CMAKE_SYSROOT ${FLUTTER_TARGET_PLATFORM_SYSROOT})[m[41m[m
[32m+[m[32m  set(CMAKE_FIND_ROOT_PATH ${CMAKE_SYSROOT})[m[41m[m
[32m+[m[32m  set(CMAKE_FIND_ROOT_PATH_MODE_PROGRAM NEVER)[m[41m[m
[32m+[m[32m  set(CMAKE_FIND_ROOT_PATH_MODE_PACKAGE ONLY)[m[41m[m
[32m+[m[32m  set(CMAKE_FIND_ROOT_PATH_MODE_LIBRARY ONLY)[m[41m[m
[32m+[m[32m  set(CMAKE_FIND_ROOT_PATH_MODE_INCLUDE ONLY)[m[41m[m
[32m+[m[32mendif()[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Define build configuration options.[m[41m[m
[32m+[m[32mif(NOT CMAKE_BUILD_TYPE AND NOT CMAKE_CONFIGURATION_TYPES)[m[41m[m
[32m+[m[32m  set(CMAKE_BUILD_TYPE "Debug" CACHE[m[41m[m
[32m+[m[32m    STRING "Flutter build mode" FORCE)[m[41m[m
[32m+[m[32m  set_property(CACHE CMAKE_BUILD_TYPE PROPERTY STRINGS[m[41m[m
[32m+[m[32m    "Debug" "Profile" "Release")[m[41m[m
[32m+[m[32mendif()[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Compilation settings that should be applied to most targets.[m[41m[m
[32m+[m[32m#[m[41m[m
[32m+[m[32m# Be cautious about adding new options here, as plugins use this function by[m[41m[m
[32m+[m[32m# default. In most cases, you should add new options to specific targets instead[m[41m[m
[32m+[m[32m# of modifying this function.[m[41m[m
[32m+[m[32mfunction(APPLY_STANDARD_SETTINGS TARGET)[m[41m[m
[32m+[m[32m  target_compile_features(${TARGET} PUBLIC cxx_std_14)[m[41m[m
[32m+[m[32m  target_compile_options(${TARGET} PRIVATE -Wall -Werror)[m[41m[m
[32m+[m[32m  target_compile_options(${TARGET} PRIVATE "$<$<NOT:$<CONFIG:Debug>>:-O3>")[m[41m[m
[32m+[m[32m  target_compile_definitions(${TARGET} PRIVATE "$<$<NOT:$<CONFIG:Debug>>:NDEBUG>")[m[41m[m
[32m+[m[32mendfunction()[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Flutter library and tool build rules.[m[41m[m
[32m+[m[32mset(FLUTTER_MANAGED_DIR "${CMAKE_CURRENT_SOURCE_DIR}/flutter")[m[41m[m
[32m+[m[32madd_subdirectory(${FLUTTER_MANAGED_DIR})[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# System-level dependencies.[m[41m[m
[32m+[m[32mfind_package(PkgConfig REQUIRED)[m[41m[m
[32m+[m[32mpkg_check_modules(GTK REQUIRED IMPORTED_TARGET gtk+-3.0)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Application build; see runner/CMakeLists.txt.[m[41m[m
[32m+[m[32madd_subdirectory("runner")[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Run the Flutter tool portions of the build. This must not be removed.[m[41m[m
[32m+[m[32madd_dependencies(${BINARY_NAME} flutter_assemble)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Only the install-generated bundle's copy of the executable will launch[m[41m[m
[32m+[m[32m# correctly, since the resources must in the right relative locations. To avoid[m[41m[m
[32m+[m[32m# people trying to run the unbundled copy, put it in a subdirectory instead of[m[41m[m
[32m+[m[32m# the default top-level location.[m[41m[m
[32m+[m[32mset_target_properties(${BINARY_NAME}[m[41m[m
[32m+[m[32m  PROPERTIES[m[41m[m
[32m+[m[32m  RUNTIME_OUTPUT_DIRECTORY "${CMAKE_BINARY_DIR}/intermediates_do_not_run"[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Generated plugin build rules, which manage building the plugins and adding[m[41m[m
[32m+[m[32m# them to the application.[m[41m[m
[32m+[m[32minclude(flutter/generated_plugins.cmake)[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# === Installation ===[m[41m[m
[32m+[m[32m# By default, "installing" just makes a relocatable bundle in the build[m[41m[m
[32m+[m[32m# directory.[m[41m[m
[32m+[m[32mset(BUILD_BUNDLE_DIR "${PROJECT_BINARY_DIR}/bundle")[m[41m[m
[32m+[m[32mif(CMAKE_INSTALL_PREFIX_INITIALIZED_TO_DEFAULT)[m[41m[m
[32m+[m[32m  set(CMAKE_INSTALL_PREFIX "${BUILD_BUNDLE_DIR}" CACHE PATH "..." FORCE)[m[41m[m
[32m+[m[32mendif()[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Start with a clean build bundle directory every time.[m[41m[m
[32m+[m[32minstall(CODE "[m[41m[m
[32m+[m[32m  file(REMOVE_RECURSE \"${BUILD_BUNDLE_DIR}/\")[m[41m[m
[32m+[m[32m  " COMPONENT Runtime)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mset(INSTALL_BUNDLE_DATA_DIR "${CMAKE_INSTALL_PREFIX}/data")[m[41m[m
[32m+[m[32mset(INSTALL_BUNDLE_LIB_DIR "${CMAKE_INSTALL_PREFIX}/lib")[m[41m[m
[32m+[m[41m[m
[32m+[m[32minstall(TARGETS ${BINARY_NAME} RUNTIME DESTINATION "${CMAKE_INSTALL_PREFIX}"[m[41m[m
[32m+[m[32m  COMPONENT Runtime)[m[41m[m
[32m+[m[41m[m
[32m+[m[32minstall(FILES "${FLUTTER_ICU_DATA_FILE}" DESTINATION "${INSTALL_BUNDLE_DATA_DIR}"[m[41m[m
[32m+[m[32m  COMPONENT Runtime)[m[41m[m
[32m+[m[41m[m
[32m+[m[32minstall(FILES "${FLUTTER_LIBRARY}" DESTINATION "${INSTALL_BUNDLE_LIB_DIR}"[m[41m[m
[32m+[m[32m  COMPONENT Runtime)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mforeach(bundled_library ${PLUGIN_BUNDLED_LIBRARIES})[m[41m[m
[32m+[m[32m  install(FILES "${bundled_library}"[m[41m[m
[32m+[m[32m    DESTINATION "${INSTALL_BUNDLE_LIB_DIR}"[m[41m[m
[32m+[m[32m    COMPONENT Runtime)[m[41m[m
[32m+[m[32mendforeach(bundled_library)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Copy the native assets provided by the build.dart from all packages.[m[41m[m
[32m+[m[32mset(NATIVE_ASSETS_DIR "${PROJECT_BUILD_DIR}native_assets/linux/")[m[41m[m
[32m+[m[32minstall(DIRECTORY "${NATIVE_ASSETS_DIR}"[m[41m[m
[32m+[m[32m   DESTINATION "${INSTALL_BUNDLE_LIB_DIR}"[m[41m[m
[32m+[m[32m   COMPONENT Runtime)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Fully re-copy the assets directory on each build to avoid having stale files[m[41m[m
[32m+[m[32m# from a previous install.[m[41m[m
[32m+[m[32mset(FLUTTER_ASSET_DIR_NAME "flutter_assets")[m[41m[m
[32m+[m[32minstall(CODE "[m[41m[m
[32m+[m[32m  file(REMOVE_RECURSE \"${INSTALL_BUNDLE_DATA_DIR}/${FLUTTER_ASSET_DIR_NAME}\")[m[41m[m
[32m+[m[32m  " COMPONENT Runtime)[m[41m[m
[32m+[m[32minstall(DIRECTORY "${PROJECT_BUILD_DIR}/${FLUTTER_ASSET_DIR_NAME}"[m[41m[m
[32m+[m[32m  DESTINATION "${INSTALL_BUNDLE_DATA_DIR}" COMPONENT Runtime)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Install the AOT library on non-Debug builds only.[m[41m[m
[32m+[m[32mif(NOT CMAKE_BUILD_TYPE MATCHES "Debug")[m[41m[m
[32m+[m[32m  install(FILES "${AOT_LIBRARY}" DESTINATION "${INSTALL_BUNDLE_LIB_DIR}"[m[41m[m
[32m+[m[32m    COMPONENT Runtime)[m[41m[m
[32m+[m[32mendif()[m[41m[m
[1mdiff --git a/linux/flutter/CMakeLists.txt b/linux/flutter/CMakeLists.txt[m
[1mnew file mode 100644[m
[1mindex 0000000..27860e8[m
[1m--- /dev/null[m
[1m+++ b/linux/flutter/CMakeLists.txt[m
[36m@@ -0,0 +1,88 @@[m
[32m+[m[32m# This file controls Flutter-level build steps. It should not be edited.[m[41m[m
[32m+[m[32mcmake_minimum_required(VERSION 3.10)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mset(EPHEMERAL_DIR "${CMAKE_CURRENT_SOURCE_DIR}/ephemeral")[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Configuration provided via flutter tool.[m[41m[m
[32m+[m[32minclude(${EPHEMERAL_DIR}/generated_config.cmake)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# TODO: Move the rest of this into files in ephemeral. See[m[41m[m
[32m+[m[32m# https://github.com/flutter/flutter/issues/57146.[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Serves the same purpose as list(TRANSFORM ... PREPEND ...),[m[41m[m
[32m+[m[32m# which isn't available in 3.10.[m[41m[m
[32m+[m[32mfunction(list_prepend LIST_NAME PREFIX)[m[41m[m
[32m+[m[32m    set(NEW_LIST "")[m[41m[m
[32m+[m[32m    foreach(element ${${LIST_NAME}})[m[41m[m
[32m+[m[32m        list(APPEND NEW_LIST "${PREFIX}${element}")[m[41m[m
[32m+[m[32m    endforeach(element)[m[41m[m
[32m+[m[32m    set(${LIST_NAME} "${NEW_LIST}" PARENT_SCOPE)[m[41m[m
[32m+[m[32mendfunction()[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# === Flutter Library ===[m[41m[m
[32m+[m[32m# System-level dependencies.[m[41m[m
[32m+[m[32mfind_package(PkgConfig REQUIRED)[m[41m[m
[32m+[m[32mpkg_check_modules(GTK REQUIRED IMPORTED_TARGET gtk+-3.0)[m[41m[m
[32m+[m[32mpkg_check_modules(GLIB REQUIRED IMPORTED_TARGET glib-2.0)[m[41m[m
[32m+[m[32mpkg_check_modules(GIO REQUIRED IMPORTED_TARGET gio-2.0)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mset(FLUTTER_LIBRARY "${EPHEMERAL_DIR}/libflutter_linux_gtk.so")[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Published to parent scope for install step.[m[41m[m
[32m+[m[32mset(FLUTTER_LIBRARY ${FLUTTER_LIBRARY} PARENT_SCOPE)[m[41m[m
[32m+[m[32mset(FLUTTER_ICU_DATA_FILE "${EPHEMERAL_DIR}/icudtl.dat" PARENT_SCOPE)[m[41m[m
[32m+[m[32mset(PROJECT_BUILD_DIR "${PROJECT_DIR}/build/" PARENT_SCOPE)[m[41m[m
[32m+[m[32mset(AOT_LIBRARY "${PROJECT_DIR}/build/lib/libapp.so" PARENT_SCOPE)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mlist(APPEND FLUTTER_LIBRARY_HEADERS[m[41m[m
[32m+[m[32m  "fl_basic_message_channel.h"[m[41m[m
[32m+[m[32m  "fl_binary_codec.h"[m[41m[m
[32m+[m[32m  "fl_binary_messenger.h"[m[41m[m
[32m+[m[32m  "fl_dart_project.h"[m[41m[m
[32m+[m[32m  "fl_engine.h"[m[41m[m
[32m+[m[32m  "fl_json_message_codec.h"[m[41m[m
[32m+[m[32m  "fl_json_method_codec.h"[m[41m[m
[32m+[m[32m  "fl_message_codec.h"[m[41m[m
[32m+[m[32m  "fl_method_call.h"[m[41m[m
[32m+[m[32m  "fl_method_channel.h"[m[41m[m
[32m+[m[32m  "fl_method_codec.h"[m[41m[m
[32m+[m[32m  "fl_method_response.h"[m[41m[m
[32m+[m[32m  "fl_plugin_registrar.h"[m[41m[m
[32m+[m[32m  "fl_plugin_registry.h"[m[41m[m
[32m+[m[32m  "fl_standard_message_codec.h"[m[41m[m
[32m+[m[32m  "fl_standard_method_codec.h"[m[41m[m
[32m+[m[32m  "fl_string_codec.h"[m[41m[m
[32m+[m[32m  "fl_value.h"[m[41m[m
[32m+[m[32m  "fl_view.h"[m[41m[m
[32m+[m[32m  "flutter_linux.h"[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32mlist_prepend(FLUTTER_LIBRARY_HEADERS "${EPHEMERAL_DIR}/flutter_linux/")[m[41m[m
[32m+[m[32madd_library(flutter INTERFACE)[m[41m[m
[32m+[m[32mtarget_include_directories(flutter INTERFACE[m[41m[m
[32m+[m[32m  "${EPHEMERAL_DIR}"[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32mtarget_link_libraries(flutter INTERFACE "${FLUTTER_LIBRARY}")[m[41m[m
[32m+[m[32mtarget_link_libraries(flutter INTERFACE[m[41m[m
[32m+[m[32m  PkgConfig::GTK[m[41m[m
[32m+[m[32m  PkgConfig::GLIB[m[41m[m
[32m+[m[32m  PkgConfig::GIO[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32madd_dependencies(flutter flutter_assemble)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# === Flutter tool backend ===[m[41m[m
[32m+[m[32m# _phony_ is a non-existent file to force this command to run every time,[m[41m[m
[32m+[m[32m# since currently there's no way to get a full input/output list from the[m[41m[m
[32m+[m[32m# flutter tool.[m[41m[m
[32m+[m[32madd_custom_command([m[41m[m
[32m+[m[32m  OUTPUT ${FLUTTER_LIBRARY} ${FLUTTER_LIBRARY_HEADERS}[m[41m[m
[32m+[m[32m    ${CMAKE_CURRENT_BINARY_DIR}/_phony_[m[41m[m
[32m+[m[32m  COMMAND ${CMAKE_COMMAND} -E env[m[41m[m
[32m+[m[32m    ${FLUTTER_TOOL_ENVIRONMENT}[m[41m[m
[32m+[m[32m    "${FLUTTER_ROOT}/packages/flutter_tools/bin/tool_backend.sh"[m[41m[m
[32m+[m[32m      ${FLUTTER_TARGET_PLATFORM} ${CMAKE_BUILD_TYPE}[m[41m[m
[32m+[m[32m  VERBATIM[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32madd_custom_target(flutter_assemble DEPENDS[m[41m[m
[32m+[m[32m  "${FLUTTER_LIBRARY}"[m[41m[m
[32m+[m[32m  ${FLUTTER_LIBRARY_HEADERS}[m[41m[m
[32m+[m[32m)[m[41m[m
