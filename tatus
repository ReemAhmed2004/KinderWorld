[32m+[m[32m#include "my_application.h"[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#include <flutter_linux/flutter_linux.h>[m[41m[m
[32m+[m[32m#ifdef GDK_WINDOWING_X11[m[41m[m
[32m+[m[32m#include <gdk/gdkx.h>[m[41m[m
[32m+[m[32m#endif[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#include "flutter/generated_plugin_registrant.h"[m[41m[m
[32m+[m[41m[m
[32m+[m[32mstruct _MyApplication {[m[41m[m
[32m+[m[32m  GtkApplication parent_instance;[m[41m[m
[32m+[m[32m  char** dart_entrypoint_arguments;[m[41m[m
[32m+[m[32m};[m[41m[m
[32m+[m[41m[m
[32m+[m[32mG_DEFINE_TYPE(MyApplication, my_application, GTK_TYPE_APPLICATION)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Called when first Flutter frame received.[m[41m[m
[32m+[m[32mstatic void first_frame_cb(MyApplication* self, FlView* view) {[m[41m[m
[32m+[m[32m  gtk_widget_show(gtk_widget_get_toplevel(GTK_WIDGET(view)));[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Implements GApplication::activate.[m[41m[m
[32m+[m[32mstatic void my_application_activate(GApplication* application) {[m[41m[m
[32m+[m[32m  MyApplication* self = MY_APPLICATION(application);[m[41m[m
[32m+[m[32m  GtkWindow* window =[m[41m[m
[32m+[m[32m      GTK_WINDOW(gtk_application_window_new(GTK_APPLICATION(application)));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Use a header bar when running in GNOME as this is the common style used[m[41m[m
[32m+[m[32m  // by applications and is the setup most users will be using (e.g. Ubuntu[m[41m[m
[32m+[m[32m  // desktop).[m[41m[m
[32m+[m[32m  // If running on X and not using GNOME then just use a traditional title bar[m[41m[m
[32m+[m[32m  // in case the window manager does more exotic layout, e.g. tiling.[m[41m[m
[32m+[m[32m  // If running on Wayland assume the header bar will work (may need changing[m[41m[m
[32m+[m[32m  // if future cases occur).[m[41m[m
[32m+[m[32m  gboolean use_header_bar = TRUE;[m[41m[m
[32m+[m[32m#ifdef GDK_WINDOWING_X11[m[41m[m
[32m+[m[32m  GdkScreen* screen = gtk_window_get_screen(window);[m[41m[m
[32m+[m[32m  if (GDK_IS_X11_SCREEN(screen)) {[m[41m[m
[32m+[m[32m    const gchar* wm_name = gdk_x11_screen_get_window_manager_name(screen);[m[41m[m
[32m+[m[32m    if (g_strcmp0(wm_name, "GNOME Shell") != 0) {[m[41m[m
[32m+[m[32m      use_header_bar = FALSE;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m#endif[m[41m[m
[32m+[m[32m  if (use_header_bar) {[m[41m[m
[32m+[m[32m    GtkHeaderBar* header_bar = GTK_HEADER_BAR(gtk_header_bar_new());[m[41m[m
[32m+[m[32m    gtk_widget_show(GTK_WIDGET(header_bar));[m[41m[m
[32m+[m[32m    gtk_header_bar_set_title(header_bar, "kinder_world");[m[41m[m
[32m+[m[32m    gtk_header_bar_set_show_close_button(header_bar, TRUE);[m[41m[m
[32m+[m[32m    gtk_window_set_titlebar(window, GTK_WIDGET(header_bar));[m[41m[m
[32m+[m[32m  } else {[m[41m[m
[32m+[m[32m    gtk_window_set_title(window, "kinder_world");[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  gtk_window_set_default_size(window, 1280, 720);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  g_autoptr(FlDartProject) project = fl_dart_project_new();[m[41m[m
[32m+[m[32m  fl_dart_project_set_dart_entrypoint_arguments([m[41m[m
[32m+[m[32m      project, self->dart_entrypoint_arguments);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  FlView* view = fl_view_new(project);[m[41m[m
[32m+[m[32m  GdkRGBA background_color;[m[41m[m
[32m+[m[32m  // Background defaults to black, override it here if necessary, e.g. #00000000[m[41m[m
[32m+[m[32m  // for transparent.[m[41m[m
[32m+[m[32m  gdk_rgba_parse(&background_color, "#000000");[m[41m[m
[32m+[m[32m  fl_view_set_background_color(view, &background_color);[m[41m[m
[32m+[m[32m  gtk_widget_show(GTK_WIDGET(view));[m[41m[m
[32m+[m[32m  gtk_container_add(GTK_CONTAINER(window), GTK_WIDGET(view));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Show the window when Flutter renders.[m[41m[m
[32m+[m[32m  // Requires the view to be realized so we can start rendering.[m[41m[m
[32m+[m[32m  g_signal_connect_swapped(view, "first-frame", G_CALLBACK(first_frame_cb),[m[41m[m
[32m+[m[32m                           self);[m[41m[m
[32m+[m[32m  gtk_widget_realize(GTK_WIDGET(view));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  fl_register_plugins(FL_PLUGIN_REGISTRY(view));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  gtk_widget_grab_focus(GTK_WIDGET(view));[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Implements GApplication::local_command_line.[m[41m[m
[32m+[m[32mstatic gboolean my_application_local_command_line(GApplication* application,[m[41m[m
[32m+[m[32m                                                  gchar*** arguments,[m[41m[m
[32m+[m[32m                                                  int* exit_status) {[m[41m[m
[32m+[m[32m  MyApplication* self = MY_APPLICATION(application);[m[41m[m
[32m+[m[32m  // Strip out the first argument as it is the binary name.[m[41m[m
[32m+[m[32m  self->dart_entrypoint_arguments = g_strdupv(*arguments + 1);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  g_autoptr(GError) error = nullptr;[m[41m[m
[32m+[m[32m  if (!g_application_register(application, nullptr, &error)) {[m[41m[m
[32m+[m[32m    g_warning("Failed to register: %s", error->message);[m[41m[m
[32m+[m[32m    *exit_status = 1;[m[41m[m
[32m+[m[32m    return TRUE;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  g_application_activate(application);[m[41m[m
[32m+[m[32m  *exit_status = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  return TRUE;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Implements GApplication::startup.[m[41m[m
[32m+[m[32mstatic void my_application_startup(GApplication* application) {[m[41m[m
[32m+[m[32m  // MyApplication* self = MY_APPLICATION(object);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Perform any actions required at application startup.[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  G_APPLICATION_CLASS(my_application_parent_class)->startup(application);[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Implements GApplication::shutdown.[m[41m[m
[32m+[m[32mstatic void my_application_shutdown(GApplication* application) {[m[41m[m
[32m+[m[32m  // MyApplication* self = MY_APPLICATION(object);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Perform any actions required at application shutdown.[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  G_APPLICATION_CLASS(my_application_parent_class)->shutdown(application);[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Implements GObject::dispose.[m[41m[m
[32m+[m[32mstatic void my_application_dispose(GObject* object) {[m[41m[m
[32m+[m[32m  MyApplication* self = MY_APPLICATION(object);[m[41m[m
[32m+[m[32m  g_clear_pointer(&self->dart_entrypoint_arguments, g_strfreev);[m[41m[m
[32m+[m[32m  G_OBJECT_CLASS(my_application_parent_class)->dispose(object);[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mstatic void my_application_class_init(MyApplicationClass* klass) {[m[41m[m
[32m+[m[32m  G_APPLICATION_CLASS(klass)->activate = my_application_activate;[m[41m[m
[32m+[m[32m  G_APPLICATION_CLASS(klass)->local_command_line =[m[41m[m
[32m+[m[32m      my_application_local_command_line;[m[41m[m
[32m+[m[32m  G_APPLICATION_CLASS(klass)->startup = my_application_startup;[m[41m[m
[32m+[m[32m  G_APPLICATION_CLASS(klass)->shutdown = my_application_shutdown;[m[41m[m
[32m+[m[32m  G_OBJECT_CLASS(klass)->dispose = my_application_dispose;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mstatic void my_application_init(MyApplication* self) {}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mMyApplication* my_application_new() {[m[41m[m
[32m+[m[32m  // Set the program name to the application ID, which helps various systems[m[41m[m
[32m+[m[32m  // like GTK and desktop environments map this running application to its[m[41m[m
[32m+[m[32m  // corresponding .desktop file. This ensures better integration by allowing[m[41m[m
[32m+[m[32m  // the application to be recognized beyond its binary name.[m[41m[m
[32m+[m[32m  g_set_prgname(APPLICATION_ID);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  return MY_APPLICATION(g_object_new(my_application_get_type(),[m[41m[m
[32m+[m[32m                                     "application-id", APPLICATION_ID, "flags",[m[41m[m
[32m+[m[32m                                     G_APPLICATION_NON_UNIQUE, nullptr));[m[41m[m
[32m+[m[32m}[m[41m[m
