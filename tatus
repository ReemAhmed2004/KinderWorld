[32m+[m[32m#include <flutter/dart_project.h>[m[41m[m
[32m+[m[32m#include <flutter/flutter_view_controller.h>[m[41m[m
[32m+[m[32m#include <windows.h>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m#include "flutter_window.h"[m[41m[m
[32m+[m[32m#include "utils.h"[m[41m[m
[32m+[m[41m[m
[32m+[m[32mint APIENTRY wWinMain(_In_ HINSTANCE instance, _In_opt_ HINSTANCE prev,[m[41m[m
[32m+[m[32m                      _In_ wchar_t *command_line, _In_ int show_command) {[m[41m[m
[32m+[m[32m  // Attach to console when present (e.g., 'flutter run') or create a[m[41m[m
[32m+[m[32m  // new console when running with a debugger.[m[41m[m
[32m+[m[32m  if (!::AttachConsole(ATTACH_PARENT_PROCESS) && ::IsDebuggerPresent()) {[m[41m[m
[32m+[m[32m    CreateAndAttachConsole();[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Initialize COM, so that it is available for use in the library and/or[m[41m[m
[32m+[m[32m  // plugins.[m[41m[m
[32m+[m[32m  ::CoInitializeEx(nullptr, COINIT_APARTMENTTHREADED);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  flutter::DartProject project(L"data");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  std::vector<std::string> command_line_arguments =[m[41m[m
[32m+[m[32m      GetCommandLineArguments();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  project.set_dart_entrypoint_arguments(std::move(command_line_arguments));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  FlutterWindow window(project);[m[41m[m
[32m+[m[32m  Win32Window::Point origin(10, 10);[m[41m[m
[32m+[m[32m  Win32Window::Size size(1280, 720);[m[41m[m
[32m+[m[32m  if (!window.Create(L"kinder_world", origin, size)) {[m[41m[m
[32m+[m[32m    return EXIT_FAILURE;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m  window.SetQuitOnClose(true);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  ::MSG msg;[m[41m[m
[32m+[m[32m  while (::GetMessage(&msg, nullptr, 0, 0)) {[m[41m[m
[32m+[m[32m    ::TranslateMessage(&msg);[m[41m[m
[32m+[m[32m    ::DispatchMessage(&msg);[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  ::CoUninitialize();[m[41m[m
[32m+[m[32m  return EXIT_SUCCESS;[m[41m[m
[32m+[m[32m}[m[41m[m
